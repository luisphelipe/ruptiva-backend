{
  "name": "ruptiva-luis-phelipe",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "quick": "NODE_ENV=test mocha --exit -r ts-node/register ./tests/setup.ts './tests/**/*test.ts'",
    "test": "NODE_ENV=test yarn migrate && mocha --exit -r ts-node/register ./tests/setup.ts './tests/**/*test.ts'",
    "ci": "NODE_ENV=ci yarn migrate && NODE_ENV=ci mocha --exit -r ts-node/register ./tests/setup.ts './tests/**/*test.ts'",
    "dev": "ts-node-dev --respawn --transpile-only --no-notify ./bin/wwwdev",
    "cpmigrations": "cp -r migrations ./dist/migrations",
    "cpmodels": "cp -r models ./dist/models",
    "cpconfig": "cp -r config ./dist/config",
    "migrate": "npx sequelize-cli db:migrate",
    "build_script": "yarn run migrate && tsc && yarn run cpmigrations && yarn run cpmodels && yarn run cpconfig",
    "build": "NODE_ENV=production yarn run build_script",
    "dev_build": "NODE_ENV=development yarn run build_script"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "rosie": "^2.0.1",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.12",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.2",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/pg": "^7.14.4",
    "@types/rosie": "^0.0.38",
    "@types/sequelize": "^4.28.9",
    "@types/sinon": "^9.0.5",
    "@types/sinon-chai": "^3.2.4"
  }
}
